{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "indent": ["error", 4],
      "max-len": ["error", 180],
      "camelcase": "off",
      "no-underscore-dangle": "off",
      "semi": ["error", "always"],
      "no-param-reassign": ["error", { "props": false }],
      "no-plusplus": "off",
      "linebreak-style": 0
  }
}
